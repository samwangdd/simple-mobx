{
  "compilerOptions": {
    "target": "ES5",
    "outDir": "./dist/",
    // "module": "commonjs",
    "sourceMap": true,
    "moduleResolution": "node",
    "strict": true,
    "experimentalDecorators": true,
    "lib": [
      "ES2017",
      "DOM"
    ],
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "allowJs": true,
    "jsx": "react",
  }
}